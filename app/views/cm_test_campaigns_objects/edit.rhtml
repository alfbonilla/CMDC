<h2 class="test_campaigns">Edit Test Campaign relation with <% @object_name %></h2>

<% labelled_tabular_form_for :cm_test_campaigns_object, @cm_test_campaigns_object,
     :html => {:id => 'cm_test_campaigns_object-form',
     :class => nil,
     :multipart => true} do |f| %>

  <%= error_messages_for 'cm_test_campaigns_object' %>

  <div class="box">
    <fieldset class="tabular"><legend>Related test_campaign Properties</legend>
      <div id="attributes" class="attributes">
        <%= render :partial => 'attributes', :locals => {:f => f} %>
      </div>
    </fieldset>
  </div>

  <%= submit_tag l(:button_create) %>
<% end %>

<% html_title "CM - Edit test_campaign-#{@object_name} Relation" %>
