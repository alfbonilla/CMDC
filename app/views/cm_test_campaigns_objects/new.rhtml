<h2 class="tests">New Test Campaign Relation <%= @object_name %></h2>

  <% labelled_tabular_form_for :cm_test_record, @cm_test_record,
       :html => {:id => 'cm_test_record-form',
       :class => nil,
       :multipart => true} do |f| %>

  <%= error_messages_for 'cm_test_record' %>

  <div class="box">
    <fieldset class="tabular"><legend>Related Test Properties</legend>
      <div id="attributes" class="attributes">
        <%= render :partial => 'attributes', :locals => {:f => f} %>
      </div>
    </fieldset>
  </div>

  <%= submit_tag l(:button_create) %>
<% end %>

<% html_title "CM - New Test Record-#{@cm_test_campaign_id} Relation" %>
