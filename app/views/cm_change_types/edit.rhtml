<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="changes">Edit Change Type</h2>

<% labelled_tabular_form_for :cm_change_type, @cm_change_type,
                             :html => {:id => 'cm_change_type-form',
                             :class => nil,
                             :multipart => true} do |f| %>

  <%= error_messages_for 'cm_change_type' %>

  <div class="box">
    <fieldset class="tabular"><legend>Change Type Properties</legend>
      <div id="attributes" class="attributes">
        <%= render :partial => 'attributes', :locals => {:f => f} %>
      </div>
    </fieldset>
  </div>
  <br />
  <%= submit_tag l(:button_submit) %>

<% end %>

<% html_title "CM - Edit Change Type #{@cm_change_type.name}" %>
