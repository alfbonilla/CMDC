<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="tests">New Relation</h2>

<% labelled_tabular_form_for :cm_tests_object, @cm_tests_object,
                   :html => {:id => 'cm_tests_object-form',
                   :class => nil,
                   :multipart => true} do |f| %>

  <%= error_messages_for 'cm_tests_object' %>

  <div class="box">
    <fieldset class="tabular"><legend>Relation Properties</legend>
      <div id="attributes" class="attributes">
        <%= render :partial => 'attributes', :locals => {:f => f} %>
      </div>
    </fieldset>
  </div>

  <%= submit_tag l(:button_create) %>

<% end %>

<% html_title "CM - New Test-TE Relation" %>