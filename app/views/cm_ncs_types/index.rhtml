<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="ncrs">Non-Conformances Types Index</h2>

<title>Index of Non-Conformances Types</title>

<% if @cm_ncs_types.empty? %>
  <p class="nodata"><%= l(:label_no_data) %></p>
<% else %>
  <%= render :partial => 'cm_ncs_types/list' %>
<% end %>

<%= link_to "Add new Non-Conformance type", {:controller => "cm_ncs_types",
  :action => 'new', :id => @project}, :class => 'icon icon-add' %>

<% html_title "CM - NC Types List" %>