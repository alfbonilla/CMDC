<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="tests">Test Classifications Index</h2>

<% if @cm_test_classifications.empty? %>
  <p class="nodata"><%= l(:label_no_data) %></p>
<% else %>
  <%= render :partial => 'cm_test_classifications/list' %>
<% end %>

<%= link_to "Add new Test Classification", {:controller => "cm_test_classifications",
  :action => 'new', :id => @project}, :class => 'icon icon-add' %>

<% html_title "CM - Test Classifications List" %>