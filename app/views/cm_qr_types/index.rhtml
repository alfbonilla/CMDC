<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="qrs">Quality Record Types Index</h2>

<title>Index of Quality Record Types</title>

<% if @cm_qr_types.empty? %>
  <p class="nodata"><%= l(:label_no_data) %></p>
