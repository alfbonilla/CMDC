<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="changes">Changes Tree Index</h2>

<div id="table" class="autoscroll">
  <% if @cm_info_changes.empty? %>
    <p class="nodata"><%= l(:label_no_data) %></p>
  <% else %>
    <%= render :partial => 'cm_changes/tree_list' %>
  <% end %>
</div>

<% html_title "CM - Changes Tree List" %>
