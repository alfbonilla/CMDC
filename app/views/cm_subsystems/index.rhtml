<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="subsystems">Subsystems Index</h2>

<% if @cm_subsystems.empty? %>
  <p class="nodata"><%= l(:label_no_data) %></p>
<% else %>
  <%= render :partial => 'cm_subsystems/list' %>
<% end %>

<%= link_to "Add new Subsystem", {:controller => "cm_subsystems",
  :action => 'new', :id => @project}, :class => 'icon icon-add' %>

<% html_title "CM - Subsystems List" %>