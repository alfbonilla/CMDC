<% labelled_tabular_form_for :cm_test, @cm_test,
                         :url => {:action => 'edit', :id => @cm_test},
                         :html => {:id => 'edit-cm_test-form',
                                   :class => nil,
                                   :multipart => true} do |f| %>

  <%= error_messages_for 'cm_test' %>

  <div class="box">
    <fieldset class="tabular"><legend>Test Properties</legend>
    <div id="attributes" class="attributes">
      <%= render :partial => 'attributes', :locals => {:f => f} %>
    </div>
  </fieldset>

  <div id="relations">
    <%= render :partial => 'relations_edit' %>
  </div>

  <fieldset><legend>Notes</legend>
  <%= text_area_tag 'notes', @notes, :cols => 60, :rows => 10, :class => 'wiki-edit' %>
  <%= wikitoolbar_for 'notes' %>
  <%= call_hook(:view_cm_tests_edit_notes_bottom, { :cm_test => @cm_test, :notes => @notes, :form => f }) %>

  </fieldset>

  <%= submit_tag l(:button_submit) %>
  </div>

<% end %>
