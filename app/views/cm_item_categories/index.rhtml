<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="iitems">Item Categories Index</h2>

<% if @cm_item_categories.empty? %>
  <p class="nodata"><%= l(:label_no_data) %></p>
<% else %>
  <%= render :partial => 'cm_item_categories/list' %>
<% end %>

<%= link_to "Add new item category", {:controller => "cm_item_categories",
      :action => 'new', :id => @project}, :class => 'icon icon-add' %>

<% html_title "CM - Categories List" %>
