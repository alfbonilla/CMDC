<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="mlogs">Maintenance Log Types Index</h2>

<% if @cm_mnt_log_types.empty? %>
<p class="nodata"><%= l(:label_no_data) %></p>
<% else %>
<%= render :partial => 'cm_mnt_log_types/list' %>
<% end %>

<%= link_to "Add new maintenance log type", {:controller => "cm_mnt_log_types",
    :action => 'new', :id => @project}, :class => 'icon icon-add' %>

<% html_title "CM - M.Log Types List" %>
