<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="tests">Test Scenarios Index</h2>

<% if @cm_test_scenarios.empty? %>
  <p class="nodata"><%= l(:label_no_data) %></p>
<% else %>
  <%= render :partial => 'cm_test_scenarios/list' %>
<% end %>

<%= link_to "Add new Test Scenario", {:controller => "cm_test_scenarios",
  :action => 'new', :id => @project}, :class => 'icon icon-add' %>

<% html_title "CM - Test Scenarios List" %>