<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="cmdocs">Document Statuses Index</h2>

<% if @cm_doc_statuses.empty? %>
  <p class="nodata"><%= l(:label_no_data) %></p>
<% else %>
  <%= render :partial => 'cm_doc_statuses/list' %>
<% end %>

<%= link_to "Add new document status", {:controller => "cm_doc_statuses",
  :action => 'new', :id => @project}, :class => 'icon icon-add' %>

<% html_title "CM - Document Statuses List" %>
