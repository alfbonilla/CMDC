<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="risks">Risk Types Index</h2>

<% if @cm_risk_types.empty? %>
  <p class="nodata"><%= l(:label_no_data) %></p>
<% else %>
  <%= render :partial => 'cm_risk_types/list' %>
<% end %>

<%= link_to "Add new risk type",
  {:action => 'new', :id => @project}, :class => 'icon icon-add' %>

<% html_title "CM - Risk Types List" %>
