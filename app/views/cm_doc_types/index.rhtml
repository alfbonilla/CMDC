<%= stylesheet_link_tag 'cmStyle', :plugin => 'redmine_cm' %>

<h2 class="cmdocs">Document Types Index</h2>

<% if @cm_doc_types.empty? %>
  <p class="nodata"><%= l(:label_no_data) %></p>
<% else %>
  <%= render :partial => 'cm_doc_types/list' %>
<% end %>

<%= link_to "Add new document type", {:controller => "cm_doc_types",
      :action => 'new', :id => @project}, :class => 'icon icon-add' %>

<% html_title "CM - Document Types List" %>
